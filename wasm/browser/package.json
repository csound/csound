{
  "name": "@csound/browser",
  "version": "0.0.1",
  "main": "./dist/libcsound.mjs",
  "type": "module",
  "keywords": [
    "csound",
    "csound-wasm",
    "wasm",
    "webassembly",
    "audio",
    "webaudio",
    "wasi"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.js'",
